<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="MultipleStatusView">
        <attr name="loadingView" format="reference" />
        <attr name="errorView" format="reference" />
        <attr name="emptyView" format="reference" />
        <attr name="networkerrorView" format="reference" />
        <attr name="contentView" format="reference" />
    </declare-styleable>

    <declare-styleable name="TopBarView">
        <attr name="colorMode" format="dimension">
            <enum name="Light" value="0" />
            <enum name="Dark" value="1" />
        </attr>
        <attr name="titleText" format="string" />
        <attr name="topbarBackground" format="color" />
        <attr name="rightText" format="string" />
        <attr name="showBack" format="boolean" />
        <attr name="leftIcon" format="reference" />
        <attr name="rightIcon" format="reference" />
        <attr name="rightSecondIcon" format="reference" />
        <attr name="rightBackground" format="reference" />
        <attr name="TopBarViewShowDivider" format="boolean" />
    </declare-styleable>

</resources>